{
  "name": "aslamhus/spotify-client",
  "description": "A PHP library for the spotify API",
  "type": "library",
  "require": {
    "guzzlehttp/psr7": "^2.6",
    "guzzlehttp/guzzle": "^7.8"
  },
  "autoload": {
    "psr-4": {
      "Aslamhus\\SpotifyClient\\": "src/"
    }
  },
  "authors": [
    {
      "name": "aslamhus",
      "email": "aslam.percy.husain@gmail.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vlucas/phpdotenv": "^5.6"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestSpotify.php",
    "test-playlist": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestPlaylist.php",
    "test-auth-code": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestAuthorizationCode.php",
    "test-artist": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestArtist.php",
    "test-search": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestSearch.php",
    "test-track": "./vendor/bin/phpunit /Applications/MAMP/htdocs/local_folder/@aslamhus/php/Spotify/tests/TestTrack.php"
  }
}
